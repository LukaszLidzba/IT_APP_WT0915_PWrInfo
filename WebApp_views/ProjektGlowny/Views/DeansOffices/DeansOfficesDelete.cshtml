@using ProjektGlowny.Models
@model DeansOfficesModel
@{
    ViewBag.Title = "Usuń dziekanat";
    Layout = "~/Views/PWrInfo/_Layout.cshtml";
}

<h1>@ViewBag.Title</h1>
<div class="title-description">
    Tutaj możesz usunąc dane dziekanatu.
</div>

<ol class="breadcrumb">
    <li><a href="#">Home</a></li>
    <li><a href="#">Dziekanaty</a></li>
    <li class="active">Usuń</li>
</ol>

@using (Html.BeginForm("DeansOfficesDelete", "DeansOffices", FormMethod.Post, new { ReturnUrl = ViewBag.ReturnUrl }))
{ 
    <div class="form-group">
       @Html.LabelFor(m =>m.Id)
       @Html.DisplayFor(m => m.Id, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Department.Name)
        @Html.DisplayFor(m => m.Department.Name, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.OpeningHours)
        @Html.DisplayFor(m => m.OpeningHours, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.AdditionalInfo)
        @Html.DisplayFor(m => m.AdditionalInfo, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Address)
        @Html.DisplayFor(m => m.Address, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.UserId)
        @Html.DisplayFor(m => m.UserId, new { @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-primary btn-lg">Usuń</button>
}



